<h1>文章列表</h1>
<% if(list.length === 0) {%>
<p>暂时没有文章</p>
<% }else{ %>
<table>
    <tr>
        <th>ID</th>
        <th>标题</th>
        <th>发表时间</th>
        <% if(logged){ %>
        <th>操作</th>
        <% } %>
    </tr>
    <% list.forEach((post)=>{ %>
    <tr>
        <td><%= post.id %> </td>
        <td><%= post.title %> </td>
        <td><%= post.time %> </td>
        <% if(logged){ %>
        <td>
            <a href="/update/<%= post.id %> ">编辑</a>
            <a href="/delete/<%= post.id %> " onclick="return confirm('确认删除吗？')">删除</a>
        </td>
        <% } %>
    </tr>
    <% }) %>
</table>
<% } %>